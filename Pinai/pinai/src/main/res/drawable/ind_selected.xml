        <shape>
            <gradient Android:angle="270" Android:endColor="@color/ind_bg" Android:startColor="@color/ind_bg" />
        </shape>
    </item>
    <item Android:state_pressed="true">
        <shape>
            <gradient Android:angle="270" Android:endColor="@color/ind_bg" Android:startColor="@color/ind_bg" />
        </shape>
    </item>
    <item>
        <shape>
            <solid Android:color="@color/white" />
        </shape>
    </item>

</selector>
